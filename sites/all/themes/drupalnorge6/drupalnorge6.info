;$Id$
name = Drupal Norge for D6
description = Tableless, multi-column, fluid width starter theme.
core = 6.x
engine = phptemplate

; Stylesheets
stylesheets[all][] = layout.css
stylesheets[all][] = drupalnorge6.css
stylesheets[all][] = blockstyle.css
stylesheets[all][] = nodestyle.css
stylesheets[all][] = commentstyle.css
stylesheets[all][] = commentstyle.css
